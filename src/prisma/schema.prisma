generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  password     String
  type         String
  active       Boolean  @default(true)
  creationDate DateTime @default(now())
  cpf          String
  cnpj         String?
  phone        String
  address      String?
  city         String?
  state        String?
  zipCode      String?
}